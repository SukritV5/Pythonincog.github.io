end)



AddCommand("febtools", function()
local Plr = game:GetService("Players").LocalPlayer
local Mouse = Plr:GetMouse()
selected = nil
oldpart = nil
 
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Delete Tool"
tool.Activated:connect(function()
if not Mouse.Target then return end
if Mouse.Target:FindFirstChild("BodyPosition") then
selected:FindFirstChild("BodyPosition"):Destroy()
end
if Mouse.Target:FindFirstChild("BodyGyro") then
selected:FindFirstChild("BodyGyro"):Destroy()
end
local del = Instance.new("BodyPosition")
del.Parent = Mouse.Target
del.P = 100000
del.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
del.Position = Vector3.new(999999999,9999999999,9999999)
end)
tool.Parent = game.Players.LocalPlayer.Backpack

tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Select Tool"
tool.Activated:connect(function()
if not Mouse.Target then return end
selected = Mouse.Target
local mem = Instance.new("SelectionBox")
mem.Parent = Mouse.Target
mem.Adornee = Mouse.Target
if oldpart then
oldpart:Destroy()
end
oldpart = mem
end)
tool.Parent = game.Players.LocalPlayer.Backpack

tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Move Tool"
tool.Activated:connect(function()
if not Mouse.Target then return end
if selected:FindFirstChild("BodyPosition") then
selected:FindFirstChild("BodyPosition"):Destroy()
end
if selected:FindFirstChild("BodyGyro") then
selected:FindFirstChild("BodyGyro"):Destroy()
end
xz = 0
local del = Instance.new("BodyPosition")
del.Parent = selected
del.P = 100000
del.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
del.Position = Vector3.new(Mouse.Hit.X,Mouse.Hit.Y,Mouse.Hit.Z)
repeat wait()
xz = xz + 1
until xz == 50 
wait(1.1)
del:Destroy()
end)
tool.Parent = game.Players.LocalPlayer.Backpack
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Anchor Tool"
tool.Activated:connect(function()
if not Mouse.Target then return end
local del = Instance.new("BodyPosition")
del.Parent = Mouse.Target
del.P = 10000
del.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
del.Position = Mouse.Target.Position
local bodyg = Instance.new("BodyGyro")
bodyg.Parent = Mouse.Target
bodyg.CFrame = Mouse.Target.CFrame
bodyg.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
end)
tool.Parent = game.Players.LocalPlayer.Backpack

