-- << Functions >> --

materializet(Search, Color3.fromRGB(255,255,255), "Search Online")
createcheckbox(Ecosia, nil, nil, function(arg) KEcosia = arg end, KEcosia)
createcheckbox(Google, nil, nil, function(arg) KGoogle = arg end, KGoogle)
