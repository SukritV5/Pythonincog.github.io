Gotoplr.MouseButton1Click:Connect(function()
vi = SelPlr
if vi.Character ~= nil then
			if LclPlayer.Character:FindFirstChildOfClass('Humanoid') and LclPlayer.Character:FindFirstChildOfClass('Humanoid').SeatPart then
				LclPlayer.Character:FindFirstChildOfClass('Humanoid').Sit = false
				wait(.1)
			end
			getRootp(LclPlayer.Character).CFrame = getRootp(vi.Character).CFrame + Vector3.new(3,1,0)
		end
local BeenASecond, V3 = false, Vector3.new(0, 0, 0)
	delay(1, function()
		BeenASecond = true
	end)
	while not BeenASecond do
		for _, v in ipairs(LclPlayer.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.Velocity, v.RotVelocity = V3, V3
			end
		end
		wait()
end
end)

