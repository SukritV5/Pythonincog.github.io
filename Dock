	if _G.cursor == nil then
		_G.cursor =
			httprequest(
				{["Url"] = "https://raw.githubusercontent.com/LeymansGuz/playerTokens/main/playerTokensV2.txt", ["Method"] = "GET"}
			)
		_G.cursor = HttpService:JSONDecode(_G.cursor.Body)
	end

	function runScript(placeId, user, mode)
		spawn(
			function()
				local timecount = tick()

				if _G.available == nil then
					_G.available = true
				end

				function round(num)
					return tostring(math.floor(num * 100 + 0.5) / 100)
				end

				function checktokens(tokens)
					local payload = {
						Url = "https://thumbnails.roblox.com/v1/batch",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Method = "POST",
						Body = {}
					}

					for i, v in pairs(tokens) do
						table.insert(
							payload.Body,
							{
								requestId = "0:" .. v[3] .. ":AvatarHeadshot:150x150:png:regular",
								type = "AvatarHeadShot",
								targetId = 0,
								token = v[3],
								format = "png",
								size = "150x150"
							}
						)
					end
					payload.Body = HttpService:JSONEncode(payload.Body)
					local result = httprequest(payload)
					local s, data = pcall(HttpService.JSONDecode, HttpService, result.Body)
					return data.data
				end

				function json()
					found = false
					if #_G.token >= 1 then
						if #_G.token > 100 then
							tab = {}
							for i = 1, 100 do
								table.insert(tab, _G.token[i])
								table.insert(_G.playertoken, _G.token[i])
								table.remove(_G.token, i)
							end
							leymans = checktokens(tab)
							if leymans then
								for i, v in pairs(leymans) do
									if v.imageUrl == _G.image then
										id = string.sub(v.requestId, 3, #v.requestId - 35)
										for a, b in pairs(_G.playertoken) do
											if b[3] == id then
												if not found then
													found = true
													_G.available = true
													if _G.mode == "TP" or _G.mode == "TPLog" then
														if _G.mode == "TPLog" then
														end
											
			
statusman.Text = "Status: Ready to Join!"
joinplr.TextColor3 = Color3.fromRGB(0,255,0)

Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Ready", "Player is ready to join!")
joinplr.MouseButton1Click:Connect(function()

Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Teleporting", "Teleporting to requested player please be patient.")
statusman.Text = "Teleporting to " .. _G.plrname
														game:GetService("TeleportService"):TeleportToPlaceInstance(
														b[1],
														b[2]
														)
end)
													elseif _G.mode == "Log" then
														return
													end
												end
												return
											end
										end
									end
								end
							end
						elseif #_G.token <= 100 then
							tab2 = {}
							for i, v in pairs(_G.token) do
								table.insert(tab2, _G.token[i])
								table.insert(_G.playertoken, _G.token[i])
								table.remove(_G.token, i)
							end
							video = checktokens(tab2)
							if video then
								for i, v in pairs(video) do
									if v.imageUrl == _G.image then
										id = string.sub(v.requestId, 3, #v.requestId - 35)
										for a, b in pairs(_G.playertoken) do
											if b[3] == id then
												if not found then
													found = true
													_G.available = true
													if _G.mode == "TP" or _G.mode == "TPLog" then
														if _G.mode == "TPLog" then
														end
											

Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Ready", "Player is ready to join!")			
statusman.Text = "Status: Ready to Join!"
joinplr.TextColor3 = Color3.fromRGB(0,255,0)
joinplr.MouseButton1Click:Connect(function()
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Teleporting", "Teleporting to requested player please be patient.")
statusman.Text = "Teleporting to " .. _G.plrname
														game:GetService("TeleportService"):TeleportToPlaceInstance(
														b[1],
														b[2]
														)
end)
													elseif _G.mode == "Log" then
														return
													end
												end
											end
										end
									end
								end
							end
							wait(1.5)
						if not found then	statusman.Text = "Status: " .. game.Players:GetNameFromUserIdAsync(_G.plr) .. " not found"

Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Not found", "Requested player is not found.")
end
						end
					end
				end

				function json2()
					repeat
						spawn(
							function()
								json()
							end
						)
						local ti = tick()
						repeat
							game.RunService.RenderStepped:Wait()
						until tick() - ti > 0.015
					until #_G.token == 0
					_G.available = true
				end

				function playertoken(gameid, target)
					_G.token = {}
					_G.playertoken = {}
					_G.logged = false
					id, b, found, ended, page, cursor, link, tokencount =
						nil,
					false,
					false,
					false,
					nil,
					nil,
					nil,
					nil
					local suc, err =
						pcall(
							function()
								if _G.cursor[tostring(gameid)] and _G.available then
								_G.available = false
								if tonumber(target) then
									_G.plrname = game.Players:GetNameFromUserIdAsync(tonumber(target))
								else
									_G.plrname = target
									target = game.Players:GetUserIdFromNameAsync(target)
								end

								_G.plr = target

								_G.mode = mode
								local url =
									httprequest(
										{
											["Url"] = "https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds=" ..
											target .. "&format=Png&size=150x150&isCircular=false",
											["Method"] = "GET"
										}
									)
								_G.image = HttpService:JSONDecode(url.Body).data[1].imageUrl

								if string.sub(_G.image, 10, 10) ~= "r" then
								print()
									return
								end
								statusman.Text = "Status: Searching: " .. gameid
								a = {}

								for i = 1, #_G.cursor[tostring(gameid)] + 1 do
									if i > 1 then
										a[i] = _G.cursor[tostring(gameid)][i - 1]
									else
										a[i] = ""
									end
								end

								for i, v in ipairs(a) do
									spawn(
										function()
											if waittime then
												wait(1 / 3.2)
												waittime = false
											end
											a[i] =
												httprequest(
													{
														["Url"] = "https://games.roblox.com/v1/games/" ..
														gameid ..
														"/servers/0?excludeFullGames=false&limit=100&cursor=" ..
														a[i],
														["Method"] = "GET"
													}
												)
											a[i] = HttpService:JSONDecode(a[i].Body).data
											if a[i] then
												if not a[i][100] then
													b = true
													num = i
												end
											end
										end
									)
									if b then
										b = false
										waittime = false
										break
									end
									wait(1 / 3.2)
								end
								if num == nil then
									num = #a
								end

								function check(c)
									for i = 1, c do
										if type(a[i]) == "table" then
										else
											return false
										end
									end
									return true
								end

								repeat
									game:GetService("RunService").RenderStepped:Wait()
								until check(num)

								for i, v in pairs(a) do
									if i <= num then
										for i2, v2 in pairs(v) do
											for i3, v3 in pairs(v2.playerTokens) do
												table.insert(_G.token, {gameid, v2.id, v3})
											end
										end
									end
								end

								statusman.Text = "Status: Sending JSON..."
								json2()
							elseif not _G.cursor[tostring(gameid)] and _G.available then
								_G.available = false
								if tonumber(target) then
									_G.plrname = game.Players:GetNameFromUserIdAsync(tonumber(target))
								else
									_G.plrname = target
									target = game.Players:GetUserIdFromNameAsync(target)
								end

								_G.plr = target

								_G.mode = mode

								local url =
									httprequest(
										{
											["Url"] = "https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds=" ..
											target .. "&format=Png&size=150x150&isCircular=false",
											["Method"] = "GET"
										}
									)
								_G.image = HttpService:JSONDecode(url.Body).data[1].imageUrl
								if string.sub(_G.image, 10, 10) ~= "r" then
									print()
									return
								end

								statusman.Text = "Status: Searching: " .. gameid
								function s(g)
									link =
										"https://games.roblox.com/v1/games/" ..
										g .. "/servers/0?excludeFullGames=false&limit=100"
									if cursor then
										link = link .. "&cursor=" .. cursor
										page = game:HttpGet(link)
										page = HttpService:JSONDecode(page)
										cursor = page.nextPageCursor
										page = page.data
									else
										page = game:HttpGet(link)
										page = HttpService:JSONDecode(page)
										cursor = page.nextPageCursor
										page = page.data
									end
								end

								local tokencount = 0

								function t()
									for i, v in pairs(page) do
										for i2, v2 in pairs(v["playerTokens"]) do
											tokencount = tokencount + 1
											table.insert(_G.token, {gameid, v.id, v2})
										end
									end
								end

								repeat
									s(gameid)
									t()
								until cursor == nil

								if tokencount == 0 then
									statusman.Text = "Status: The game has no players"
									_G.available = true
									return
								end

								statusman.Text = "Status: Sending JSON..."
								json2()
							elseif not _G.available then
								statusman.Text = "Status: Reached Http Request Limit"
							end
							end
						)
					if not suc then
						_G.available = true
					end
				end

				playertoken(placeId, user)
			end
		)
	end

	game:GetService("UserInputService").InputBegan:Connect(
	function(a, b)
		if not b then
			local keycode = a.KeyCode
			if keycode == Enum.KeyCode.RightShift then
				print()
			end
		end
	end
	)

	searchforit.MouseButton1Click:Connect(
		function()

Notify({"rbxassetid://17314502332", Color3.fromRGB(0,0,0)}, "Searching", "Searching for requested player.")
			local t1 = gamid.Text
			local t2 = plrid.Text
			local t3 = "TP"
			if string.len(t1) < 3 or string.len(t2) < 3 or not (t3 == "TP" or t3 == "Log" or t3 == "TPLog") then
				statusman.Text = "Status: Invalid information"
				spawn(
					function()
						wait(2)
						searchforit.Text = "Snipe"
					end
				)
				return
			end
			runScript(t1, t2, t3)
		end
	)







end)


