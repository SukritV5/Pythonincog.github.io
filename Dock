notviewf = true
viewing = nil
Viewplr.MouseButton1Click:Connect(function()
if notviewf == true then
notviewf = false
Viewplr.Text = "Unview"
local Player = SelPlr
		if viewDied then
			viewDied:Disconnect()
			viewChanged:Disconnect()
		end
		viewing = Player
		workspace.CurrentCamera.CameraSubject = viewing.Character
		local function viewDiedFunc()
			repeat wait() until Player.Character ~= nil and getRootp(Player.Character)
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewDied = Player.CharacterAdded:Connect(viewDiedFunc)
		local function viewChangedFunc()
			workspace.CurrentCamera.CameraSubject = viewing.Character
		end
		viewChanged = workspace.CurrentCamera:GetPropertyChangedSignal("CameraSubject"):Connect(viewChangedFunc)
else
notviewf = true
Viewplr.Text = "View"
viewing = nil
viewDied:Disconnect()
viewChanged:Disconnect()
	workspace.CurrentCamera.CameraSubject = LclPlayer.Character
end
end)

function updcontent()
if SelPlr then
local con = Players:GetUserThumbnailAsync(SelPlr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
PlrIcon.Image = con
PlrName.Text = SelPlr.Name
Plrid.Text = "ID: " .. SelPlr.UserId
local ag = tostring(SelPlr.Team)
Plrteam.Text = "Team: " .. ag
PlrAge.Text = "Age: " .. SelPlr.AccountAge
notviewf = true
Viewplr.Text = "View"
end
end

updcontent()

function updlist()
for i,v in pairs(PlrsHolder:GetDescendants()) do
v:Destroy()
end

task.wait()

for i,v in pairs(Players:GetPlayers()) do
local PlrBtn = create('TextButton',{
ZIndex = 8;
Text = v.Name .. "\n[" .. v.DisplayName .. "]";
Font = Enum.Font.SourceSansLight;
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(1,0,0,24);
Position = UDim2.new(0.001,0,0,0+prevp);
Parent = PlrsHolder;
})

PlrBtn.MouseButton1Click:Connect(function()
SelPlr = Players:FindFirstChild(v.Name)
print(SelPlr)
updcontent()
end)
task.wait()
prevp = prevp + 30
end
end

updlist()
Players.PlayerAdded:Connect(function(player)
prevp = 0
updlist()
end)

Players.PlayerRemoving:Connect(function(player)
prevp = 0
updlist()
end)



end)


tabHolder.Scripts.MouseButton1Click:Connect(function()
local windo = Window.new({width = 250, height = 310, name='Scripts', icon='rbxassetid://18900459968'})

function RunLS(str)
local success, errormsg = pcall(function()
loadstring(game:HttpGet(str), true)()
end)

if success == true then
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Success", "Script Successfully ran.")
else
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Failed", "Script failed to run could be Deleted/Patched/Broken or unsupported by your exploit.")
end

end

