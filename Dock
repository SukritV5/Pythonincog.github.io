Parent = workspace.CurrentCamera;
Size = 0;
})

local blurLevel = 0

RunService.Stepped:Connect(function()
local nm,nm2 = blurEffect.Size,workspace.CurrentCamera.FieldOfView
blurEffect.Size = nm + ((blurLevel - nm) * .1)
end)

