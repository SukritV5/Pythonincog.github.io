setcmdp.MouseButton1Down:connect(function()
local window = Window.new({width = 165, height = 100, name='Set Prefix', icon='rbxassetid://17382166939'})
local pLabel = create('TextLabel',{
ZIndex = 8;
Text = "Prefix";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.45,-60,0.05,0);
Parent = window.Content;
})
local preBox  = create('TextBox',{
ZIndex = 8;
Text = tostring(prefix);
ClearTextOnFocus = false;
TextSize = 28;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.55,0,0.05,0);
Parent = window.Content;
})

preBox.FocusLost:connect(function(enter)
if enter then
prefix = tostring(preBox.Text)
updatesave()
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Prefix changed", "Your prefix has been changed.")
end
end)
end)

setcmdkey.MouseButton1Click:Connect(function()

local window = Window.new({width = 165, height = 100, name='Set Cmd Key', icon='rbxassetid://17382166939'})
local changebtn = create('TextButton',{
ZIndex = 8;
Text = "Set Keybind";
Size = UDim2.new(.45,0,.6,0);
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.05,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})
local crtbtn = create('TextLabel',{
ZIndex = 8;
Text = tostring(barbindd[1][1]):sub(14);
Size = UDim2.new(.45,0,.6,0);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Position = UDim2.new(0.5,0,0.15,0);
Font = Enum.Font.SourceSansLight;
TextScaled = true;
Parent = window.Content;
})

local function urm()
toggleable = false
local asy = UserInputService.InputBegan:wait()
if asy.UserInputType == Enum.UserInputType.Keyboard then
crtbtn.Text = "Press A Key"
barbindd[1][1] = a.KeyCode
ToggleKeyCmd = barbindd[1][1]
crtbtn.Text = tostring(barbindd[1][1]):sub(14);
wait()
Notify({"rbxassetid://17301803826", Color3.fromRGB(0,0,0)}, "Keybind changed", "Your keybind has been changed.")
toggleable = true;
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your device is unsupported with keybind\nUse the button on the corner left to toggle krunox")
end
end
changebtn.MouseButton1Down:connect(urm)
end)

