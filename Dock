fpsunlo.MouseButton1Click:Connect(function()

local window = Window.new({width = 165, height = 100, name='Set FPS', icon='rbxassetid://0'})
local fpslabel = create('TextLabel',{
ZIndex = 8;
Text = "FPS";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = 1;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.45,-60,0.05,0);
Parent = window.Content;
})
local fpslevelbox  = create('TextBox',{
ZIndex = 8;
Text = "60";
ClearTextOnFocus = false;
TextSize = 28;
MultiLine = false;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.new(1,1,1);
Size = UDim2.new(0,60,0,60);
Position = UDim2.new(0.55,0,0.05,0);
Parent = window.Content;
})

fpslevelbox.Changed:connect(function()
fpslevelbox.Text = fpslevelbox.Text:gsub("%D","")
end)

fpslevelbox.FocusLost:connect(function(enter)
if enter then

if setfpscap and type(setfpscap) == "function" then
if tonumber(fpslevelbox.Text) > 0 then
setfpscap(tonumber(fpslevelbox.Text))
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Number must be greater than 0")
end
else
Notify({"rbxassetid://14549962093", Color3.fromRGB(0,0,0)}, "Error", "Your exploit does not support this function (setfpscap)")
end
end
end)

end)

