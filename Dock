	cleardrawcache()
end)

-- WebSocket

test("WebSocket", {})

test("WebSocket.connect", {}, function()
	local types = {
		Send = "function",
		Close = "function",
		OnMessage = {"table", "userdata"},
		OnClose = {"table", "userdata"},
	}
	local ws = WebSocket.connect("ws://echo.websocket.events")
	assert(type(ws) == "table" or type(ws) == "userdata", "Did not return a table or userdata")
	for k, v in pairs(types) do
		if type(v) == "table" then
			assert(table.find(v, type(ws[k])), "Did not return a " .. table.concat(v, ", ") .. " for " .. k .. " (a " .. type(ws[k]) .. ")")
		else
			assert(type(ws[k]) == v, "Did not return a " .. v .. " for " .. k .. " (a " .. type(ws[k]) .. ")")
		end
	end
	ws:Close()
end)
-- end of unc test
end

function joinlogsstart()

local window = Window.new({width = 390, height = 303, name='Join logs', icon='rbxassetid://70935639316572'})

local doom = create('ScrollingFrame',{BottomImage='rbxasset://textures/ui/Scroll/scroll-middle.png', ScrollBarImageTransparency=0.7,CanvasPosition=Vector2.new(0, 0),CanvasSize=UDim2.new(0, 0, 10, 0),MidImage='rbxasset://textures/ui/Scroll/scroll-middle.png',ScrollBarThickness=6,ScrollingEnabled=true,TopImage='rbxasset://textures/ui/Scroll/scroll-middle.png',Active=false,AnchorPoint=Vector2.new(0, 0),BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=1,BorderColor3=Color3.fromRGB(0.105882, 0.164706, 0.207843),BorderSizePixel=0,ClipsDescendants=true,Draggable=false,Position=UDim2.new(0.025, 0, 0, 0),Rotation=0,Selectable=true,Size=UDim2.new(1, 0, 0, 600),SizeConstraint=Enum.SizeConstraint.RelativeXY,Visible=true,ZIndex=8,Name='chat',Parent = window.Content})

clogen = true

local onoffbtn = create('TextButton',{
Font = Enum.Font.SourceSansLight;
ZIndex = 8;
Text = "Enabled";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.fromRGB(0,255,0);
Size = UDim2.new(.475,0,0,24);
Position = UDim2.new(0.025,0,0,240);
Parent = window.Content;
})

local exportfile = create('TextButton',{
Font = Enum.Font.SourceSansLight;
ZIndex = 8;
Text = "Export to TXT File";
TextScaled = true;
BackgroundColor3 = Color3.new(1,1,1);
BackgroundTransparency = .9;
TextColor3 = Color3.fromRGB(255,255,255);
Size = UDim2.new(.475,0,0,24);
Position = UDim2.new(0.505,0,0,240);
Parent = window.Content;
})

Fixname = function(name)
    return tostring(name):gsub("[*\\?:<>|]+", ""):sub(1, 175)
end

exportfile.MouseButton1Click:Connect(function()

if iswritefile() then
		if #doom:GetChildren() > 0 then
Notify({"rbxassetid://1669787447", Color3.fromRGB(0,0,0)}, "Downloading", "Saving file to your exploits workspace folder")
			local Nameplace = Fixname(MarketplaceService:GetProductInfo(game.PlaceId).Name)
			local writelogsk = 'Krunox JoinLogs [' ..Nameplace.. ']\n'
			for _, child in pairs(doom:GetChildren()) do
				writelogsk = writelogsk.. '\n' ..child.Text
			end
			local writelogsFilek = tostring(writelogsk)
			local Alreadyfile = 0
			local function filename()
				local file
				pcall(function() file = readfile(Nameplace..' Join Logs ('..Alreadyfile..').txt') end)
				if file then

Alreadyfile = Alreadyfile+1
					filename()
				else
					writefile(Nameplace..' JoinLogs KRUNOX ['..Alreadyfile..'].txt', writelogsFilek)
				end
			end
			filename()
Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Exported", "Join Logs saved.")
		end
	else
		Notify({"rbxassetid://14549990176", Color3.fromRGB(0,0,0)}, "Unsupported", "Your exploit does not support this function [writefile]")
	end

end)

onoffbtn.MouseButton1Click:Connect(function()
if clogen == true then
clogen = false
onoffbtn.TextColor3 = Color3.fromRGB(255,0,0)
onoffbtn.Text = "Disabled"
else
clogen = true
onoffbtn.TextColor3 = Color3.fromRGB(0,255,0)
onoffbtn.Text = "Enabled"
end
end)


local pout = 0
function output(plr, msg)
if clogen then

if #doom:GetChildren() >= 2546 then
			scroll_2:ClearAllChildren()
		end
		local AALS = 0

		for i,v in pairs(doom:GetChildren()) do
			if v then
				AALS = v.Size.Y.Offset + AALS
			end
			if not v then
				AALS = 0
			end
		end


	local colour = Color3.fromRGB(255,255,255)
	
	if string.sub(msg, 1,1) == ":" or string.sub(msg,1,1) == ";" then colour = Color3.fromRGB(255,255,255) elseif string.sub(msg,1,2) == "/w" or string.sub(msg,1,7) == "/whisper" or string.sub(msg,1,5) == "/team" or string.sub(msg,1,2) == "/t" then colour = Color3.fromRGB(255,0,0) else colour = Color3.fromRGB(255,255,255) end
	
 	local label = CreateInstance('TextLabel',{Font=Enum.Font.SourceSansLight,FontSize=Enum.FontSize.Size24,Text="" .. plr.Name .. "" ..  msg,TextColor3=colour,TextScaled=false,TextSize=23,TextStrokeColor3=Color3.fromRGB(0, 0, 0),TextStrokeTransparency=1,TextTransparency=0,TextWrapped=false,TextXAlignment=Enum.TextXAlignment.Left,TextYAlignment=Enum.TextYAlignment.Top,Active=false,AnchorPoint=Vector2.new(0, 0),BackgroundColor3=Color3.fromRGB(255,255,255),BackgroundTransparency=1,BorderColor3=Color3.fromRGB(0.105882, 0.164706, 0.207843),BorderSizePixel=1,ClipsDescendants=false,Draggable=false,Position=UDim2.new(0.025,0,0,0 + pout),Rotation=0,Selectable=false,Size=UDim2.new(0, 271, 0,
100),Visible=true,ZIndex=8,Name="h",Parent = doom})

doom.CanvasSize = UDim2.new(0,0,0,AALS+label.TextBounds.Y-10)
pout = pout + 20
doom.CanvasPosition = Vector2.new(0,doom.CanvasPosition.Y+label.TextBounds.Y-4)
end
end

Players.ChildAdded:Connect(function(plr)
	if plr:IsA("Player") then
			output(plr, "has joined the game")
	end
end)
end


